{
  "bugs": "https://github.com/AstarNetwork/astar.js/issues",
  "engines": {
    "yarn": "^1.10.1"
  },
  "homepage": "https://github.com/AstarNetwork/astar.js#readme",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/AstarNetwork/astar.js.git"
  },
  "sideEffects": false,
  "version": "0.2.8",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "generate": "yarn update-metadata && yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package @astar-network/astar-types/interfaces --input ./packages/types/src/interfaces --endpoint ./metadata.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package @astar-network/astar-types/interfaces --endpoint ./metadata.json --output ./packages/types/src/interfaces",
    "build": "node ./scripts/build-release.js",
    "clean": "polkadot-dev-clean-build",
    "lint": "polkadot-dev-run-lint",
    "postinstall": "polkadot-dev-yarn-only",
    "run-example": "ts-node -r tsconfig-paths/register examples/example.ts",
    "test": "polkadot-dev-run-test --coverage --passWithNoTests",
    "update-metadata": "curl -s -H \"Content-Type: application/json\" -d '{\"id\":\"1\", \"jsonrpc\":\"2.0\", \"method\": \"state_getMetadata\", \"params\":[]}' https://rpc.shibuya.astar.network > metadata.json"
  },
  "dependencies": {
    "ts-proto": "^1.82.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.23.2",
    "@open-web3/dev-config": "^0.1.14",
    "@open-web3/util": "^2.0.1",
    "@polkadot/api": "10.9.1",
    "@polkadot/api-augment": "10.9.1",
    "@polkadot/api-contract": "10.9.1",
    "@polkadot/api-derive": "10.9.1",
    "@polkadot/dev": "^0.67.178",
    "@polkadot/rpc-core": "10.9.1",
    "@polkadot/typegen": "10.9.1",
    "@polkadot/types": "10.9.1",
    "@polkadot/types-codec": "10.9.1",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-node": "^8.6.1",
    "tsconfig-paths": "^3.12.0"
  },
  "resolutions": {
    "bn.js": "4.12",
    "typescript": "^4.6.2"
  }
}
